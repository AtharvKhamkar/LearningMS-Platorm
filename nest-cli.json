{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auth-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/learning-ms",
  "projects": {
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "course-service": {
      "type": "application",
      "root": "apps/course-service",
      "entryFile": "main",
      "sourceRoot": "apps/course-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/course-service/tsconfig.app.json"
      }
    },
    "enrollment-service": {
      "type": "application",
      "root": "apps/enrollment-service",
      "entryFile": "main",
      "sourceRoot": "apps/enrollment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/enrollment-service/tsconfig.app.json"
      }
    },
    "mail-worker": {
      "type": "application",
      "root": "apps/mail-worker",
      "entryFile": "main",
      "sourceRoot": "apps/mail-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mail-worker/tsconfig.app.json"
      }
    },
    "user-management-service": {
      "type": "application",
      "root": "apps/user-management-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-management-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-management-service/tsconfig.app.json"
      }
    }
  }
}